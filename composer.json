{
  "name": "frc/payrix",
  "description": "Payrix api library for laravel apps",
  "type": "library",
  "authors": [
    {
      "name": "Ethan Brace",
      "email": "eab@frc.org"
    }
  ],
  "require": {
    "php": "^7.4",
    "laravel/framework": "^8.0",
    "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
    "ext-json": "*"
  },
  "require-dev": {
    "pestphp/pest-plugin-laravel": "^1.1",
    "pestphp/pest": "^1.16",
    "orchestra/testbench": "^6.15"
  },
  "autoload": {
    "psr-4": {
      "Frc\\Payrix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Frc\\Payrix\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Frc\\Payrix\\PayrixServiceProvider"
      ]
    }
  }
}
